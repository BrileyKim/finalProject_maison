<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="notice">
	<select id="selectNoticeList" resultType="notice">
		SELECT * FROM NOTICE ORDER BY NOTICENO DESC
	</select>
	<select id="selectCount" resultType="_int">
		SELECT COUNT(*) FROM NOTICE 
		
	</select>
	
	<insert id="insertNotice" parameterType="map" >
		INSERT INTO NOTICE VALUES(NOTICESEQ.NEXTVAL,#{noticeTitle},#{noticeContent},default,default,default)
	</insert>
	
	<select id="selectNoticeOne" parameterType="_int" resultType="notice">
		SELECT * FROM NOTICE WEHERE NOTICENO =#{noticeNo}
	</select>
</mapper>
